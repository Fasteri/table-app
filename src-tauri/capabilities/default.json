{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:default",
    {
      "identifier": "shell:allow-open",
      "allow": [
        { "path": "$APPDATA/**" },
        { "path": "$HOME/Library/Application Support/**" }
      ]
    }
  ]
}
